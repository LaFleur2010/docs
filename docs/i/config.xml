<?xml version="1.0" encoding="utf-8" ?>
<cu3er>
	<settings>
	    <auto_play>
           <defaults symbol="circular"/>
           <tweenIn x="895" y="45" width="30" height="30" tint="0x20282e" alpha="0.5"/>
           <tweenOver alpha="1"/>
        </auto_play>
		
    	<prev_button>
			<defaults round_corners="5,5,5,5"/>
			<tweenOver tint="0xFFFFFF" scaleX="1.1" scaleY="1.1"/>
			<tweenOut tint="0x000000" />
		</prev_button>
		
    	<prev_symbol>
			<tweenOver tint="0x000000" />			
		</prev_symbol>
		
		<description>
         <defaults round_corners="6, 6, 6, 6" heading_text_size="14" heading_text_color="0xffffff" paragraph_text_size="6" paragraph_text_color="0xe1e1e1"/>
         <tweenIn x="100" y="235" alpha="0.30"/>
         <tweenOver alpha="0.5"/>
        </description>
		
    	<next_button>
			<defaults round_corners="3,3,3,3"/>			
			<tweenOver tint="0xFFFFFF"  scaleX="1.1" scaleY="1.1"/>
			<tweenOut tint="0x000000" />
		</next_button>
		
    	<next_symbol>
			<tweenOver tint="0x000000" />
		</next_symbol>	
			
	</settings>    

	<slides>
		<slide>
    		<url>images/slide_1.jpg</url>
			<description>
      		<heading></heading>
   			</description>
        </slide>
		
		<!-- transición de cambio entre la primera y segunda diapositiva -->
        <transition num="3" slicing="vertical" direction="down"/>
		
        <slide>
       		<url>images/slide_2.jpg</url>
       		<description>
       		<!--<link target="_blank">http://www.mgyt.com</link> -->
      		<heading></heading>
     		<paragraph></paragraph>
   			</description>
		</slide>	
		
			
		<!-- transición de cambio entre la segunda y tercera diapositiva -->
        <transition num="4" direction="right" shader="flat" />
		
		<slide>
            <url>images/slide_3.jpg</url>
			<description>
       		<!--<link target="_blank">http://www.mgyt.com</link> -->
      		<heading></heading>
     		<paragraph></paragraph>
   			</description>
        </slide>
		
		<!-- transitions properties defined in transitions template -->
		<!--<slide>
       		<url>images/slide_8.jpg</url>
        </slide>
		
		<transition num="6" slicing="vertical" direction="up" shader="flat" delay="0.05" z_multiplier="4" />
        <slide>
       		<url>images/slide_9.jpg</url>
        </slide>-->
				
	</slides>
</cu3er>

